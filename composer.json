{
    "name":              "cache/doctrine-adapter",
    "description":       "A PSR-6 Cache Implementation using Doctrine",
    "type":              "library",
    "license":           "MIT",
    "minimum-stability": "stable",
    "keywords":          ["cache", "psr-6", "doctrine"],
    "homepage":          "https://github.com/php-cache/psr-6-cache",
    "authors":           [
        {
            "name":     "Aaron Scherer",
            "email":    "aequasi@gmail.com",
            "homepage": "https://github.com/aequasi"
        },
        {
            "name":     "Tobias Nyholm",
            "email":    "tobias.nyholm@gmail.com",
            "homepage": "https://github.com/nyholm"
        }
    ],
    "require":           {
        "psr/cache": "dev-master"
    },
    "provides": {
        "psr/cache-implementation": "1.0.0"
    },
    "repositories":      [
        {
            "type": "git",
            "url":  "https://github.com/php-fig/cache.git"
        }
    ],
    "require-dev":       {
        "phpunit/phpunit": "~5.1"
    },
    "autoload":          {
        "psr-4": {
            "Cache\\Doctrine\\": "src/"
        }
    },
    "autoload-dev":      {
        "psr-4": {
            "Cache\\Doctrine\\Tests\\": "tests/"
        }
    }
}
